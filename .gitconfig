[user]
    name = "Mitchell Bowden"
    email = mitchellbowden@gmail.com
[github]
    user = msbmsb
[core]
    autocrlf = input
[alias]
    st = status
    ci = commit
    b = branch
    co = checkout
    df = diff
    lg = log -p        
    ls = ls-files
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan    
[branch]
    autosetupmerge = true
